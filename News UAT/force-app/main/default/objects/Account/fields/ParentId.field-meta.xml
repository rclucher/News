<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ParentId</fullName>
    <inlineHelpText>Select if this Account is a subsidiary of another organisation. If not, tick No Parent Account checkbox</inlineHelpText>
    <lookupFilter>
        <active>true</active>
        <filterItems>
            <field>Account.RecordType.Name</field>
            <operation>notEqual</operation>
            <value>Private Party</value>
        </filterItems>
        <infoMessage>The Parent Account cannot be a Private Party</infoMessage>
        <isOptional>false</isOptional>
    </lookupFilter>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>true</trackHistory>
    <type>Hierarchy</type>
</CustomField>
