<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Customer_Discount_Percent__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Pre_Discount_Price__c) || (Pre_Discount_Price__c == 0), 0, ROUND((1 - (Rolling_12_Months_Print__c / Pre_Discount_Price__c)) * 100, 0) / 100)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Customer Discount Percent</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
