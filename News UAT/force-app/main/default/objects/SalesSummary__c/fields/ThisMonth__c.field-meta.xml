<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ThisMonth__c</fullName>
    <description>This date is used for the report to calculate which records belong to this month</description>
    <externalId>false</externalId>
    <formula>DATE(Year__c, VALUE(RIGHT(MonthId__c, 2)) ,1)</formula>
    <label>This Month</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
