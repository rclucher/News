<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_cannot_be_in_the_Past</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISCHANGED(End_Date__c),(End_Date__c &lt; TODAY()),(End_Date__c&lt;Start_Date__c))</errorConditionFormula>
    <errorMessage>End Date cannot be in the past.</errorMessage>
</ValidationRule>
