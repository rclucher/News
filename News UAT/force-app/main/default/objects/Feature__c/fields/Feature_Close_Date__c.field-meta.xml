<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Feature_Close_Date__c</fullName>
    <description>Determine the date the Feature should be set to Closed</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Feature_End_Date__c), Feature_Date__c, Feature_End_Date__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Feature Close Date</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
