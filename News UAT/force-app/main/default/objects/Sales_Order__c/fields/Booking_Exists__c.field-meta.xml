<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Booking_Exists__c</fullName>
    <description>To indicate whether a Sales Order has Bookings</description>
    <externalId>false</externalId>
    <formula>IF( Number_of_Bookings__c  &gt; 0 ,  IMAGE(&#39;/img/msg_icons/confirm16.png&#39;, &#39;Yes&#39;) , IMAGE(&#39;/img/msg_icons/error16.png&#39;, &#39;No&#39;))</formula>
    <inlineHelpText>Does this Sales Order have a Booking/Bookings?</inlineHelpText>
    <label>Booking Exists</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
